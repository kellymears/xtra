{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaFeatures": {
        "jsx": true,
        "modules": true
    }
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "comma-dangle": [1,"never"],
    "no-cond-assign": [1,"always"],
    "no-constant-condition": 1,
    "no-control-regex": 1,
    "no-debugger": 1,
    "no-dupe-args": 1,
    "no-dupe-keys": 1,
    "no-duplicate-case": 1,
    "no-empty-character-class": 1,
    "no-empty": 1,
    "no-ex-assign": 1,
    "no-extra-boolean-cast": 1,
    "no-extra-semi": 1,
    "no-func-assign": 1,
    "no-inner-declarations": [1,"both"],
    "no-invalid-regexp": 1,
    "no-irregular-whitespace": 1,
    "no-negated-in-lhs": 1,
    "no-obj-calls": 1,
    "no-regex-spaces": 1,
    "no-sparse-arrays": 1,
    "no-unreachable": 1,
    "use-isnan": 1,
    "valid-typeof": 1,
    "accessor-pairs": 1,
    "complexity": [1,4],
    "curly": [1,"multi"],
    "default-case": 1,
    "dot-notation": 1,
    "dot-location": [1,"object"],
    "no-alert": 0,
    "no-labels": 1,
    "no-eq-null": 1,
    "no-eval": 1,
    "no-extend-native": 1,
    "no-extra-bind": 1,
    "no-fallthrough": 1,
    "no-floating-decimal": 1,
    "no-invalid-this": 1,
    "no-lone-blocks": 1,
    "no-loop-func": 1,
    "no-multi-str": 1,
    "no-native-reassign": 1,
    "no-new-func": 1,
    "no-new-wrappers": 1,
    "no-octal-escape": 1,
    "no-octal": 1,
    "no-redeclare": 1,
    "no-return-assign": 1,
    "no-script-url": 1,
    "no-self-compare": 1,
    "no-sequences": 1,
    "yoda": [1,"never",{}],
    "no-delete-var": 1,
    "no-undef": 1,
    "no-unused-vars": 1,
    "callback-return": 1,
    "array-bracket-spacing": [1,"never",{}],
    "brace-style": [1,"1tbs",{"allowSingleLine":true}],
    "camelcase": [1,{"properties":"always"}],
    "comma-spacing": 1,
    "comma-style": 1,
    "computed-property-spacing": 1,
    "eol-last": 1,
    "indent": [2, 2],
    "key-spacing": [1,{"align":"value"}],
    "lines-around-comment": [1,{"beforeBlockComment":true}],
    "no-mixed-spaces-and-tabs": 1,
    "no-multiple-empty-lines": [1,{"max":1}],
    "object-curly-spacing": [1,"never",{}],
    "padded-blocks": [1,"never"],
    "quotes": [2, "single", "avoid-escape"],
    "space-in-parens": [2,"never"]
  }
}
